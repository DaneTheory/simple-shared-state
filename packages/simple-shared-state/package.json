{
  "name": "simple-shared-state",
  "version": "4.1.5",
  "description": "Easily share state between components using a no-frills observable object API",
  "workspaces": true,
  "module": "src/index.js",
  "main": "dist/simple-shared-state.es6.umd.js",
  "unpkg": "dist/simple-shared-state.es6.umd.js",
  "homepage": "https://simplesharedstate.com",
  "scripts": {
    "lint": "eslint .",
    "test": "jest --watchAll",
    "doc": "jsdoc -c ./jsdoc.json -t templates/",
    "build": "webpack",
    "size": "npx size-limit"
  },
  "size-limit": [
    {
      "path": "dist/simple-shared-state.es6.umd.js",
      "limit": "10 KB"
    }
  ],
  "keywords": [
    "shared state",
    "redux",
    "mobx",
    "react",
    "context",
    "context api"
  ],
  "author": "Rob Christian <rm.rf.etc@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/rm-rf-etc/simple-shared-state"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@size-limit/preset-small-lib": "^3.0.0",
    "babel-loader": "^8.0.6",
    "docdash": "^1.1.1",
    "esm": "^3.2.25",
    "jest": "^24.9.0",
    "redux": "^4.0.5",
    "taffydb": "^2.7.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
